issn = {00097330},
journal = {Circulation research},
pages = {40--50},
pmid = {13975592},
publisher = {Lippincott Williams {\&} Wilkins},
title = {{Resistivity of body tissues at low frequencies.}},
url = {http://ahajournals.org},
volume = {12},
year = {1963}
}
