file = {:D$\backslash$:/Clouds/Library/Article/Heart/Impedance/Phillips2017{\_}Article{\_}StrokeVolumeMonitoringNovelCon.pdf:pdf},
issn = {2167-6275},
journal = {Current Anesthesiology Reports},
keywords = {Algorithms,Concepts,Continuous wave Doppler,Haemodynamic monitoring,Stroke volume,algorithms,cardio,concepts,continuous wave doppler,haemodynamic monitoring,stroke volume,sv},
mendeley-tags = {cardio,sv},
number = {4},
pages = {387--398},
publisher = {Current Anesthesiology Reports},
title = {{Stroke Volume Monitoring: Novel Continuous Wave Doppler Parameters, Algorithms and Advanced Noninvasive Haemodynamic Concepts}},
volume = {7},
year = {2017}
}
