journal = {IEEE Transactions on Biomedical Circuits and Systems},
keywords = {Amplitude modulation,Gaussian mixture regression (GMR),cardio,electrical impedance plethysomgraphy (EIS),lung volume,physical activity monitoring,remote health monitoring systems,reo,respiration rate,spiro,spirometry,support vector regression},
mendeley-tags = {cardio,reo,spiro},
