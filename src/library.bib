keywords = {Automated external defibrillator (AED),ECG analysis,Sinus rhythm (SR),Ventricular fibrillation (VF),Ventricular fibrillation detection,algorithm,ecg},
mendeley-tags = {algorithm,ecg},
pages = {1--15},
title = {{Reliability of old and new ventricular fibrillation detection algorithms for automated external defibrillators}},
volume = {4},
year = {2005}
